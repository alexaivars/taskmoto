{
  "name": "@taskmoto/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev:generate": "yarn generate",
    "dev:server": "NODE_EXTRA_CA_CERTS=$(grep NODE_EXTRA_CA_CERTS .env | cut -d '=' -f2) ts-node --project tsconfig.server.json server",
    "dev": "yarn:generate && yarn dev:server",
    "generate": "NODE_EXTRA_CA_CERTS=$(grep NODE_EXTRA_CA_CERTS .env | cut -d '=' -f2) graphql-codegen --require dotenv/config --config codegen.yml",
    "start": "NODE_EXTRA_CA_CERTS=$(grep NODE_EXTRA_CA_CERTS .env | cut -d '=' -f2) NODE_ENV=production node --require dotenv/config dist/index.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@taskmoto/ui": "1.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "next": "10.1.3",
    "polished": "^4.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.2.3",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.5",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-react-apollo": "^2.2.4",
    "@types/http-proxy": "^1.17.5",
    "@types/jscodeshift": "^0.11.0",
    "@types/react": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "babel-plugin-styled-components": "^1.12.0",
    "jscodeshift": "^0.11.0",
    "ts-node": "^9.1.1"
  }
}
