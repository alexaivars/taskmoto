{
  "name": "@taskmoto/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev:ui": "chokidar --initial \"./src/ui/{,**/}*.@(ts|tsx)\" -i \"./src/ui/index.ts\" -i \"./src/ui/{,**/}.stories.@(ts|tsx)\" -c \"yarn generate:ui\"",
    "dev:generate": "yarn generate && scripts/generateIndex.sh",
    "dev:server": "NODE_EXTRA_CA_CERTS=/Users/alex/projects/taskmoto/scripts/certs/RootCA.pem ts-node --project tsconfig.server.json server",
    "dev": "yarn:generate && yarn dev:server",
    "generate:ui": "scripts/generateIndex.sh",
    "generate": "NODE_EXTRA_CA_CERTS=$(grep NODE_EXTRA_CA_CERTS .env | cut -d '=' -f2) graphql-codegen --require dotenv/config --config codegen.yml",
    "start": "NODE_EXTRA_CA_CERTS=$(grep NODE_EXTRA_CA_CERTS .env | cut -d '=' -f2) NODE_ENV=production node --require dotenv/config dist/index.js"
  },
  "dependencies": {
    "@apollo/client": "^3.4.7",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "next": "11.1.2",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.0.1",
    "@graphql-codegen/fragment-matcher": "^3.0.0",
    "@graphql-codegen/introspection": "2.0.0",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.0.0",
    "@graphql-codegen/typescript-document-nodes": "2.0.0",
    "@graphql-codegen/typescript-operations": "2.0.1",
    "@graphql-codegen/typescript-react-apollo": "3.0.0",
    "@storybook/react": "^6.3.7",
    "@svgr/webpack": "^5.5.0",
    "@types/http-proxy": "^1.17.7",
    "@types/jscodeshift": "^0.11.2",
    "@types/react": "^17.0.16",
    "@types/styled-components": "^5.1.12",
    "babel-plugin-styled-components": "^1.13.2",
    "chokidar-cli": "^3.0.0",
    "jscodeshift": "0.13.0",
    "ts-graphql-plugin": "^2.1.3",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "typescript-styled-plugin": "^0.18.1"
  }
}
